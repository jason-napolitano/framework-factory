#!/usr/bin/env php
<?php require __DIR__.'/../vendor/autoload.php';

$command = $argv[1] ?? null;
$name = $argv[2] ?? null;

if ($command === 'build:provider' && $name) {
    new \FrameworkFactory\Commands\MakeProvider($name)->handle();
}

if ($command === 'build:accessor' && $name) {
    new \FrameworkFactory\Commands\MakeAccessor($name)->handle();
}